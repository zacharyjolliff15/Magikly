<script type="text/javascript">

    var Tealium = {
      init: function (tag) {
        this.defaultTag = tag;
        utag_data = JSON.parse(JSON.stringify(tag));
      },
      buildTag: function (tagOrFn) {
        var tag = tagOrFn;

        if (typeof tagOrFn == 'function') {
          tag = {};
          tagOrFn(tag);
        }


        var target = {};

        for (var name in this.defaultTag) {
          target[name] = this.defaultTag[name];
        }

        for (var name in tag) {
          target[name] = tag[name];
        }

        console.log(JSON.parse(JSON.stringify(target)));

        return utag_data = target;
      },
      view: function (tag) {
        utag.view(this.buildTag(tag));
      },
      link: function (tag) {
        utag.link(tag);
      },
      underscore: function (text) {
        return text.replace(/-/g, '_');
      }
    };


    Tealium.init({
      content_visitor: 'home',
      page_name: 'home',
      lang: 'en',
      sign_in: 0,
      nav_category: 'home',
      sub_category: '',
      over21: 0
    });

    Tealium.processTag = (function (initialized) {
      return function (tag) {
        var route = window.location.hash.split('/');

        if (route[1]) {
          tag.page_name = route[route.length-1];
          tag.nav_category = route[1];
          // tag.nav_category = 'home';
          tag.sub_category = route[2] || '';

          if (!initialized) {
            tag.user_navigation = 'deep_link';
          }
        }

        initialized = true;

        if (route[1] == 'products') {
          tag.content_visitor = 'product';

          if (route[3]) {
            var product = route[route.length-1];
            tag.product_tier_0 = product.indexOf('diet') != -1 ? 'diet' : 'regular';
            tag.product_tier_1 = Tealium.underscore(route[route.length-2]);
            tag.product = Tealium.underscore(product);
          } else if (route[2]) {
            tag.product_tier_0 = Tealium.underscore(route[route.length-1]);
          }
        }

        if (route[1] == 'our-sparkling-heritage') {
          tag.content_visitor = 'our_sparkling_heritage';
        }
      };
    })();

    Tealium.buildTag(Tealium.processTag);


